# What3Words API Configuration
WHAT3WORDS_API_KEY=your_what3words_api_key_here

# AWS S3 Configuration
# Get these from your AWS Console > IAM > Access Keys
AWS_ACCESS_KEY_ID=AKIA1234567890EXAMPLE
AWS_SECRET_ACCESS_KEY=your_40_character_secret_access_key_here
AWS_S3_BUCKET=your-unique-bucket-name
AWS_S3_REGION=eu-north-1

# Flask Configuration (Optional)
SECRET_KEY=your_secret_key_here

# Database Configuration (Optional)
DATABASE_URL=your_database_url_here

# Email Configuration (Optional)
MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_USE_TLS=True
MAIL_USERNAME=your_email_username_here
MAIL_PASSWORD=your_email_password_here
MAIL_DEFAULT_SENDER=your_sender_email_here

# Application URLs (Optional)
LIVE_APP_URL=https://your-app-url.herokuapp.com

# Push Notification Keys (Optional)
VAPID_PUBLIC_KEY=your_vapid_public_key_here
VAPID_PRIVATE_KEY=your_vapid_private_key_here